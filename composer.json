{
  "name": "dolcy/router_walk",
  "description": "A small, modern router demo.",
  "license": "MIT",
  "authors": [{
    "name": "Shaughn Dolcy",
    "email": "sdolcy@gmail.com"
  }],
  "require": {
    "php": ">=7.2",
    "vlucas/phpdotenv": "^3.6",
    "illuminate/database": "^6.5",
    "league/route": "^4.3",
    "zendframework/zend-diactoros": "^2.1",
    "zendframework/zend-httphandlerrunner": "^1.1",
    "psr/http-factory": "^1.0",
    "middlewares/whoops": "^1.2",
    "nesbot/carbon": "^2.26",
    "monolog/monolog": "^2.0",
    "rdlowrey/auryn": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "RouterWalk\\": "src/",
      "RouterWalk\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4",
    "codeception/codeception": "4.0.x-dev"
  },
  "minimum-stability": "dev",
  "scripts": {
    "start": "php -S localhost:8080 -t public",
    "test": [
      "@phpunit"
    ],
    "phpunit": "php vendor/bin/phpunit"
  },
  "config": {
    "process-timeout": 0
  }
}
