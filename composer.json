{
  "name": "akian0/php_router_one",
  "description": "A small, modern router demo.",
  "license": "MIT",
  "authors": [{
    "name": "Shaughn Dolcy",
    "email": "sdolcy@gmail.com"
  }],
  "require": {
    "php": ">=7.2",
    "league/container": "^3.3",
    "vlucas/phpdotenv": "^3.6",
    "illuminate/database": "^6.5",
    "league/route": "^4.3",
    "zendframework/zend-diactoros": "^2.1",
    "zendframework/zend-httphandlerrunner": "^1.1",
    "psr/http-factory": "^1.0",
    "middlewares/whoops": "^1.2",
    "nesbot/carbon": "^2.26",
    "guzzlehttp/guzzle": "^6.4",
    "monolog/monolog": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "RouterApp\\": "src/",
      "RouterApp\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4"
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public",
    "test": [
      "@phpunit"
    ],
    "phpunit": "php vendor/bin/phpunit"
  }
}
